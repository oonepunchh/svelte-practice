{
    "editor.codeActionsOnSave": {
        "source.fixAll": true,
        "addMissingImports": true
    },
    "eslint.validate": [
        "javascript",
        "typescript",
        "svelte"
    ],
    "prettier.enable": true,
    "editor.formatOnSave": true,
    "[svelte]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    }
}